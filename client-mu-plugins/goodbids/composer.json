{
	"name": "viget/goodbids.org",
	"description": "The WordPress project set up for the goodbids.org main plugin.",
	"type": "project",
	"authors": [
		{
			"name": "Viget"
		}
	],
	"homepage": "https://github.com/wpcomvip/goodbids",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"optimize-autoloader": true,
		"sort-packages": true,
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"cweagans/composer-patches": true,
			"oomphinc/composer-installers-extender": true
		}
	},
	"autoload": {
		"psr-4": {
			"GoodBids\\": [
				"src/classes/",
				"src/classes/*",
				"src/classes/*/*"
			]
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		},
		{
			"type":"composer",
			"url":"https://connect.advancedcustomfields.com"
		},
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"require": {
		"php": "^8.1",
		"ext-mbstring": "*",
		"composer/installers": "^1 || ^2",
		"cweagans/composer-patches": "^1.7",
		"oomphinc/composer-installers-extender": "^2.0",
		"vlucas/phpdotenv": "^5.5",
		"wpackagist-plugin/accessibility-checker": "^1.6",
		"wpackagist-plugin/cookie-law-info": "^3.1",
		"wpackagist-plugin/svg-support": "^2.5",
		"wpackagist-plugin/user-switching": "^1.7",
		"wpackagist-plugin/woocommerce": "^8.4",
		"wpackagist-plugin/woocommerce-gateway-stripe": "^7.7",
		"wpengine/advanced-custom-fields-pro": "*"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^1.0.0",
		"squizlabs/php_codesniffer": "^3.7",
		"wp-coding-standards/wpcs": "^3.0"
	},
	"extra": {
		"installer-types": [
			"bower-asset",
			"npm-asset"
		],
		"installer-paths": {
			"../{$name}": [
				"type:wordpress-muplugin"
			],
			"../../plugins/{$name}": [
				"type:wordpress-plugin"
			],
			"../../themes/{$name}": [
				"type:wordpress-theme"
			]
		},
		"patches": {}
	}
}
