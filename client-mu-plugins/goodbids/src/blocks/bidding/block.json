{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "goodbids/bidding",
  "title": "Bidding",
  "category": "widgets",
  "description": "Display bidding widget",
  "supports": {
    "html": false,
    "renaming": false
  },
  "textdomain": "default",
  "attributes": {
    "auctionId": {
      "type": "number"
    },
    "socketUrl": {
      "type": "string"
    },
    "shareUrl": {
      "type": "string"
    },
    "rewardUrl": {
      "type": "string"
    },
    "bidUrl": {
      "type": "string"
    },
    "freeBidUrl": {
      "type": "string"
    },
    "createAccountUrl": {
      "type": "string"
    },
    "logInUrl": {
      "type": "string"
    },
    "startTime": {
      "type": "string"
    }
  },
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js"
}
